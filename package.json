{
  "name": "es6-webpack-boilerplate",
  "version": "1.0.0",
  "description": "es6 webpack boilerplate with unit test/formatter/linter setup",
  "author": "Gongxia Chen",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=dev webpack-dev-server --progress --mode development --config webpack.config.dev.js",
    "build": "webpack -p --progress --mode production --config webpack.config.build.js",
    "test": "jest --coverage"
  },
  "devDependencies": {
    "@babel/core": "7.2.0",
    "@babel/preset-env": "^7.1.5",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "8.0.4",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "clean-webpack-plugin": "1.0.0",
    "cross-env": "5.2.0",
    "css-loader": "2.0.0",
    "eslint": "^5.9.0",
    "file-loader": "2.0.0",
    "html-webpack-plugin": "3.2.0",
    "jest": "^23.6.0",
    "node-sass": "4.11.0",
    "open-browser-webpack-plugin": "0.0.5",
    "prettier": "^1.15.2",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1",
    "url-loader": "1.1.2",
    "webpack": "4.27.1",
    "webpack-cli": "3.1.2",
    "webpack-dev-server": "3.1.10",
    "webpack-merge": "4.1.5"
  }
}
